<form action="<%= users_url %>" method='post'>

<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">

<h1>Sign Up</h1>

<label for="firstName">First Name</label>
<input type="text" name="user[firstName]" id="firstName">

<br>

<label for="lastName">Last Name</label>
<input type="text" name="user[lastName]" id="lastName">

<br>

<label for="birthDate">Date of Birth</label>
<input type="date" name="user[birthDate]" id="birthDate">

<br>

<label for="email">Email</label>
<input type="text" name="user[email]" id="email">

<br>

<label for="password">Password</label>
<input type="password" name="user[password]" id="password">

<br>
<br>

<input type='submit' value="Sign Up">

</form>

<a href="<%= new_session_url %>">Sign In</a>


