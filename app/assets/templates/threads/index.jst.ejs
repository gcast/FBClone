<ul class="threads-list group">
	<% threads.each(function(thread) {  %>
		<a href="#/threads/<%= thread.id %>">
			<li>
				<header>
					<% id = thread.get("user_one") === FBClone.current_user.id ? thread.get("user_two") : thread.get("user_one") %>
				
					<%= FBClone.current_user.friends.get(id).get("first_name")  %>
					<%= FBClone.current_user.friends.get(id).get("last_name")  %>
				</header>				

				<div>
					<%= thread.messages.last().get("message") %>
					<%= prettyDate(thread.messages.last().get("created_at")) %>
				</div>
			</li>
		</a>
		<br>
	<% }) %>
</ul>



